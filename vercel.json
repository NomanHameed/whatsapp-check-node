{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ],
  "functions": {
    "app.js": {
      "maxDuration": 300
    }
  },
  "git": {
    "deploymentEnabled": {
      "vercel": true,
      "main": false
    }
  }
}